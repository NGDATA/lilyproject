<?xml version="1.0"?>
<indexer xmlns:b="org.lilyproject.bookssample">
  <records>
    <record matchNamespace="b" matchName="Book" matchVariant="*" vtags="last"/>
  </records>

  <fields>
    <field name="title" value="b:title"/>

    <field name="authors" value="b:authors=>b:name"/>
  </fields>

</indexer>
