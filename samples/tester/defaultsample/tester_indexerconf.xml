<indexer xmlns:t="org.lilyproject.tools.tester">
  <recordFilter>
    <includes>
      <include recordType="t:RT1" vtags="last"/>
      <include recordType="t:RT2" vtags="last"/>
      <include recordType="t:RT3" vtags="last"/>
      <include recordType="t:RTSub" vtags="last"/>
    </includes>
  </recordFilter>

  <fields>
    <field name="t$field1" value="t:field1"/>

    <field name="t$field2" value="t:field2"/>

    <field name="t$field3" value="t:field3"/>

    <field name="t$field4" value="t:field4"/>

    <field name="t$field5" value="t:field5"/>

    <field name="t$field6" value="t:field6"/>

    <field name="t$field7" value="t:field7"/>

    <field name="t$field8" value="t:field8"/>

    <field name="t$field9" value="t:field9"/>

    <field name="t$field10" value="t:field10"/>
  </fields>

</indexer>
